<div class="wrapper">
  <br />
  <div>
    <h3>My Blogs Dashboard:</h3>
  </div>
</div>

<div class="grid">

  {{#each blogs as |blog|}}
    <div id="dashboard-blogs" class="content">
      <section class="card">
        <a href="/blogs/{{id}}">
            <div class="card-body-simple">
            <div>
                <h1>{{blog_title}}</h1>
                <h2>Posted on: {{format_date date}} / By: {{username}}</h2>
          </div>
       </a>

        </div>
      </section>
    </div>
  {{/each}}
</div>